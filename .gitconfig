[user]
	email = cloud760607@hotmail.com
	name = MRSC

[alias]
  gl = log -16 --color=always --all --graph --topo-order --pretty='format: %Cgreen%h%Creset %s%Cred%d%Creset%n'                                 
  merge = merge --no-ff
  up = merge --ff-only

  co = checkout
  ci = commit -s

  st = status
  stat = status

  b = branch
  br = branch

  hist = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=relative
  fo = fetch origin
  fu = fetch upstream
  po = push origin
[color]
  ui = true
  diff = auto
  status = auto
  branch = auto
  log = auto
  ui = auto
[apply]
  whitespace = nowarn
[core]
    editor = /usr/bin/vim
